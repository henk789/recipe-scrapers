/* Page tagline styling */
.page-tagline {
	width: 75%;
	margin-left: auto;
	margin-right: auto;
}
.page-tagline p:last-child {
	margin-bottom: 0px;
}

/* hide entry summary in archive pages */
.generate-columns-container .inside-article .entry-summary {
	display: none;
}

/* Boxzilla Newsletter PopUp styling */
.boxzilla-newsletter .email-label {
	display: none;
}
.boxzilla-newsletter .mc4wp-form {
	text-align: center;
}
.boxzilla-newsletter .mc4wp-form .newsletter-button input {
	background-color: var(--byan-accent-1);
}
.boxzilla-newsletter .mc4wp-form p:last-child label {
	font-size: 15px;
}
.boxzilla-newsletter .mc4wp-form p:last-child {
	margin-bottom: 0px;
}
.boxzilla-newsletter .mc4wp-form p:last-child a:hover {
	text-decoration: underline;
}

/* Insta Footer fix */
#footer-widgets .footer-widgets-container {
	max-width: 100%;
}
.footer-widget-1 p {
	margin-bottom: 0px;
}

/* newsletter home page */
.mc4wp-hide-language {
	display: none;
}
.mailchimp-newsletter .mc4wp-form-fields {
	display: flex;
	align-items: center;
	justify-content: center;
	flex-direction: row;
	flex-wrap: wrap;
}
.mailchimp-newsletter .mc4wp-form-fields > p:last-child {
	width: 100%;
	text-align: center;
	margin-bottom: 0px;
	font-size: 16px;
}
.mailchimp-newsletter .mc4wp-form-fields .email-label {
	display: none;
}

/* archive post img styling */
.post-image-above-header .inside-article div.post-image {
	margin-bottom: 15px;
}

/* wprm recipe time styling (archives) */
.inside-article .entry-header .wprm-recipe-total-time-container, .custom-query-loop-container .wprm-recipe-total-time-container {
	display: flex;
	margin-bottom: 5px;
	font-size: 16px;
	align-items: center;
}
.inside-article .entry-header .wprm-recipe-total-time-container .wprm-recipe-time, .custom-query-loop-container .wprm-recipe-total-time-container .wprm-recipe-time {
